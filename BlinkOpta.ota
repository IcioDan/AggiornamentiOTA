// Sketch base per Arduino Opta
void setup() {
  Serial.begin(115200);
  while (!Serial) {}
  Serial.println("âœ… Firmware OTA caricato con successo!");
  pinMode(LED_BUILTIN, OUTPUT);
}

void loop() {
  digitalWrite(LED_BUILTIN, HIGH);
  delay(500);
  digitalWrite(LED_BUILTIN, LOW);
  delay(500);
}